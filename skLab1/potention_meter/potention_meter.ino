void setup()
{
  pinMode(A0,INPUT);
  pinMode(D2,INPUT_PULLUP);
  Serial.begin(9600);
}

void loop()
{
  int var = analogRead(A0);
  if(!digitalRead(D2))
  {
    Serial.println(var);
    delay(500);
  }
  
}